digraph go_sales_train {
	Country [label="{ Country|+CountryCode: Integer\l+Country: String\l+Language: String\l+CurrencyName: String\l }" shape=record]
	OrderDetails [label="{ OrderDetails|+OrderDetailCode: Integer\l+OrderNumber: Integer\l+ProductNumber: Integer\l+Quantity: Integer\l+UnitCost: Real\l+UnitPrice: Real\l+UnitSalePrice: Real\l }" shape=record]
	OrderHeader [label="{ OrderHeader|+OrderNumber: Integer\l+RetailerName: String\l+RetailerSiteCode: Integer\l+RetailerContactCode: Integer\l+SalesStaffCode: Integer\l+SalesBranchCode: Integer\l+OrderDate: String\l+OrderMethodCode: Integer\l }" shape=record]
	OrderMethod [label="{ OrderMethod|+OrderMethodCode: Integer\l+OrderMethodEn: String\l }" shape=record]
	Product [label="{ Product|+ProductNumber: Integer\l+IntroductionDate: String\l+ProductTypeCode: Integer\l+ProductionCost: Real\l+Margin: Real\l+ProductImage: String\l+Language: String\l+ProductName: String\l+Description: String\l }" shape=record]
	ProductLine [label="{ ProductLine|+ProductLineCode: Integer\l+ProductLineEn: String\l }" shape=record]
	ProductType [label="{ ProductType|+ProductTypeCode: Integer\l+ProductLineCode: Integer\l+ProductTypeEn: String\l }" shape=record]
	RetailerSite [label="{ RetailerSite|+RetailerSiteCode: Integer\l+RetailerCode: Integer\l+Address1: String\l+Address2: String\l+City: String\l+Region: String\l+CountryCode: Integer\l+ActiveIndicator: Integer\l }" shape=record]
	ReturnReason [label="{ ReturnReason|+ReturnReasonCode: Integer\l+ReturnDescriptionEn: String\l }" shape=record]
	ReturnedItem [label="{ ReturnedItem|+ReturnCode: Integer\l+ReturnDate: String\l+OrderDetailCode: Integer\l+ReturnReasonCode: Integer\l+ReturnQuantity: Integer\l }" shape=record]
	SalesBranch [label="{ SalesBranch|+SalesBranchCode: Integer\l+Address1: String\l+Address2: String\l+City: String\l+Region: String\l+CountryCode: Integer\l }" shape=record]
	SalesStaff [label="{ SalesStaff|+SalesStaffCode: Integer\l+FirstName: String\l+LastName: String\l+PositionEn: String\l+WorkPhone: String\l+Extension: Integer\l+Fax: String\l+Email: String\l+DateHired: String\l+SalesBranchCode: Integer\l }" shape=record]
	OrderDetails -> OrderHeader [label=OrderNumber]
	OrderDetails -> Product [label=ProductNumber]
	OrderHeader -> OrderMethod [label=OrderMethodCode]
	OrderHeader -> SalesStaff [label=SalesStaffCode]
	OrderHeader -> SalesBranch [label=SalesBranchCode]
	RetailerSite -> Country [label=CountryCode]
	ReturnedItem -> OrderDetails [label=OrderDetailCode]
	ReturnedItem -> ReturnReason [label=ReturnReasonCode]
	SalesStaff -> SalesBranch [label=SalesBranchCode]
	Product -> ProductType [label=ProductTypeCode]
	ProductType -> ProductLine [label=ProductLineCode]
	RetailerSite -> OrderHeader [label=RetailerSiteCode]
}
